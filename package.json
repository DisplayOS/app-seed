{
  "name": "enplug-seed",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.0.0"
  },
  "config": {
    "ports": {
      "server": 50000
    },
    "build_env": "prod",
    "aws": {
      "buckets": [
        "TODO: insert.staging.bucket.name.here",
        "TODO: insert.production.bucket.name.here"
      ],
      "s3": {
        "region": "us-west-2",
        "sslEnabled": true,
        "apiVersion": "2016-05-01",
        "params": {
          "ACL": "public-read"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node scripts/init.js",
    "clean:dashboard": "node scripts/clean.js 'dist/dashboard/*'",
    "clean:dash": "npm run clean:dashboard",
    "clean:app": "node scripts/clean.js 'dist/app/*'",
    "clean": "node scripts/clean.js 'dist/dashboard/*' 'dist/app/*'",
    "prebuild:dashboard": "npm run clean:dashboard",
    "build:dashboard": "webpack --config webpack.config.dashboard.js",
    "build:dash": "npm run build:dashboard",
    "prebuild:app": "npm run clean:app",
    "build:app": "webpack --config webpack.config.app.js",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.config.js",
    "predev:dashboard": "npm run build:dashboard",
    "dev:dashboard": "webpack-dev-server --content-base dist/ --hot --inline --colors --port $npm_package_config_ports_server --config webpack.config.dashboard.js --enplug-seed:build_env=local",
    "dev:dash": "npm run dev:dashboard",
    "predev:app": "npm run clean:app",
    "dev:app": "webpack-dev-server --content-base dist/ --hot --inline --colors --port $npm_package_config_ports_server --config webpack.config.app.js --enplug-seed:build_env=local",
    "predev": "npm run build",
    "dev": "webpack-dev-server --content-base dist/ --hot --inline --colors --port $npm_package_config_ports_server --config webpack.config.js --enplug-seed:build_env=local",
    "bump": "node scripts/bump.js",
    "prerelease": "npm-run-all -s bump build",
    "release": "node scripts/release.js",
    "rel": "node scripts/release.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@enplug/dashboard-sdk": "^3.0.3",
    "@enplug/dashboard-sdk-utils": "^2.1.0",
    "@enplug/player-sdk": "0.0.2",
    "angular": "~1.4.10",
    "angular-route": "^1.5.8",
    "babel-plugin-transform-runtime": "^6.8.0",
    "lodash": "~3.10.0"
  },
  "devDependencies": {
    "@enplug/babel-preset": "^1.0.0",
    "@enplug/eslint-config": "^1.0.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "boxcutter": "^1.2.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "inquirer": "^1.0.2",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.7.0",
    "npm-run-all": "^2.1.0",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "s3": "^4.4.0",
    "sass-loader": "^3.2.0",
    "semver": "^5.1.0",
    "shelljs": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-combine-loaders": "^2.0.0",
    "webpack-dev-server": "^1.14.1"
  }
}
