{
  "name": "example-app",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.0.0"
  },
  "config": {
    "ports": {
      "dashboard": 40000,
      "app": 40001
    },
    "destination": "enplug/example-app/",
    "targets": [
      "dashboard",
      "app"
    ],
    "aws": {
      "buckets": [
          "apps.enplug.in",
          "apps.enplug.com"
      ],
      "s3": {
        "cache": false,
        "overwrite": true,
        "region": "us-west-2",
        "sslEnabled": true,
        "apiVersion": "2016-05-01",
        "params": {
          "ACL": "public-read"
        }
      }
    }
  },
  "scripts": {
    "version": "node scripts/version-bump.js",
    "build": "node scripts/build.js",
    "dev": "node scripts/build.js --dev-server",
    "release": "node scripts/build.js --build-for-release"
  },
  "dependencies": {
    "@enplug/dashboard-sdk": "^4.0.7",
    "@enplug/dashboard-sdk-utils": "^2.1.9",
    "@enplug/player-sdk": "0.0.7",
    "angular": "~1.4.10",
    "angular-route": "^1.5.8",
    "babel-plugin-transform-runtime": "^6.8.0",
    "lodash": "~3.10.0",
    "moment": "^2.14.1",
    "youtube-player": "^3.0.4"
  },
  "devDependencies": {
    "@enplug/babel-preset": "^1.0.0",
    "@enplug/eslint-config": "^1.0.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "boxcutter": "^1.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "inquirer": "^1.0.2",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.7.0",
    "npm-run-all": "^2.1.0",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "s3": "^4.4.0",
    "sass-loader": "^3.2.0",
    "semver": "^5.1.0",
    "shelljs": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-combine-loaders": "^2.0.0",
    "webpack-dev-server": "^1.14.1"
  }
}
