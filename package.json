{
  "name": "enplug-seed",
  "version": "2.0.0",
  "private": true,
  "config": {
    "ports": {
      "dashboard": 50000,
      "app": 50001,
      "browsersync": 50002
    },
    "build_env": "prod",
    "aws": {
      "s3": {
        "region": "us-east-1",
        "sslEnabled": true,
        "apiVersion": "2016-05-01",
        "params": {
          "ACL": "public-read",
          "bucket": "TODO: insert.bucket.name.here"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node scripts/init.js",
    "clean:dashboard": "node scripts/clean.js 'dist/dashboard/*'",
    "clean:dash": "npm run clean:dashboard",
    "clean:app": "node scripts/clean.js 'dist/app/*'",
    "clean": "node scripts/clean.js 'dist/dashboard/*' 'dist/app/*'",
    "prebuild:dashboard": "npm run clean:dashboard",
    "build:dashboard": "webpack --config webpack.config.dashboard.js",
    "build:dash": "npm run build:dashboard",
    "prebuild:app": "npm run clean:app",
    "build:app": "webpack --config webpack.config.app.js",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.config.js",
    "dev:dash": "npm run dev:dashboard",
    "dev:dashboard": "npm run build:dashboard --enplug-seed:build_env=local -- --watch",
    "dev:app": "npm run build:app --enplug-seed:build_env=local -- --watch",
    "dev": "npm run build --enplug-seed:build_env=local -- --watch",
    "prerelease": "npm-run-all -s bump build",
    "release": "node scripts/release.js",
    "rel": "node scripts/release.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bump": "node scripts/bump.js"
  },
  "dependencies": {
    "angular": "~1.4.10",
    "babel-plugin-transform-runtime": "^6.8.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "aws-sdk": "^2.3.11",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "bower": "^1.7.2",
    "boxcutter": "^1.2.0",
    "grunt": "^0.4.5",
    "grunt-aws": "^0.6.1",
    "grunt-bower-concat": "^0.6.0",
    "grunt-bump": "^0.7.0",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-uglify": "^0.11.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-html2js": "^0.3.5",
    "grunt-ng-annotate": "^1.0.1",
    "grunt-postcss": "^0.7.1",
    "grunt-processhtml": "^0.3.9",
    "grunt-prompt": "^1.3.3",
    "grunt-replace": "^0.11.0",
    "grunt-sass": "^1.1.0",
    "html-loader": "^0.4.3",
    "inquirer": "^1.0.2",
    "jit-grunt": "^0.9.1",
    "load-grunt-config": "^0.19.1",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "npm-run-all": "^2.1.0",
    "rimraf": "^2.5.2",
    "semver": "^5.1.0",
    "shelljs": "^0.7.0",
    "webpack": "^1.13.0",
    "webpack-combine-loaders": "^2.0.0"
  }
}
