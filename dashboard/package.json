{
  "name": "enplug-appseed-dashboard",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "node ../fix-node-modules.js",
    "ng": "ng",
    "build": "enplug-build --configuration=development --nooffline",
    "build:staging": "enplug-build --configuration=staging --nooffline",
    "build:prod": "enplug-build --configuration=production --nooffline",
    "release": "enplug-release --configuration=development --nooffline",
    "release:staging": "enplug-release --configuration=staging --nooffline",
    "release:prod": "enplug-release --configuration=production --nooffline",
    "start": "ng serve --configuration=development --port=50000",
    "start:staging": "ng serve --configuration=staging --port=50000",
    "start:prod": "ng serve --configuration=production --port=50000",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "ng-xi18n --i18nFormat=xlf"
  },
  "config": {
    "destination": "/enplug/appseed/dashboard/",
    "aws": {
      "buckets": [
        "dev-apps.enplug.in",
        "apps.enplug.in",
        "apps.enplug.com"
      ],
      "s3": {
        "cache": false,
        "overwrite": true,
        "region": "us-west-2",
        "sslEnabled": true,
        "apiVersion": "2016-05-01",
        "params": {
          "ACL": "public-read"
        }
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/platform-server": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@biesbjerg/ngx-translate-po-http-loader": "^3.1.0",
    "@enplug/dashboard-sdk": "^4.7.2",
    "@ngx-translate/core": "^10.0.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.3",
    "intl": "^1.2.5",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "^7.0.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@enplug/scripts": "^1.8.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "karma-mocha-reporter": "^2.2.5",
    "protractor": "^5.4.0",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.2.4"
  }
}
