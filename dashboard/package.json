{
  "name": "dashboard-seed",
  "version": "3.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "enplug-build --configuration=development --nooffline",
    "build:staging": "enplug-build --configuration=staging --nooffline",
    "build:prod": "enplug-build --configuration=production --nooffline",
    "release": "enplug-release --configuration=development --nooffline",
    "release:staging": "enplug-release --configuration=staging --nooffline",
    "release:prod": "enplug-release --configuration=production --nooffline",
    "release:translations": "enplug-release-translations",
    "start": "enplug-serve --configuration=local --host=localhost.enplug.in --port=50000",
    "start:dev": "enplug-serve --configuration=development --host=localhost.enplug.in --port=50000",
    "start:staging": "enplug-serve --configuration=staging --host=localhost.enplug.in --port=50000",
    "start:prod": "enplug-serve --configuration=production --host=localhost.enplug.in --port=50000",
    "test": "ng test",
    "lint": "ng lint && npm run lint:stylelint",
    "lint:fix": "ng lint --fix && npm run lint:stylelint -- --fix",
    "lint:stylelint": "stylelint \"src/**/*.scss\"",
    "e2e": "ng e2e",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find"
  },
  "config": {
    "destination": "/enplug/appseed/dashboard/",
    "aws": {
      "buckets": [
        "dev-apps.enplug.in",
        "apps.enplug.in",
        "apps.enplug.com"
      ],
      "s3": {
        "cache": false,
        "overwrite": true,
        "region": "us-west-2",
        "sslEnabled": true,
        "apiVersion": "2016-05-01",
        "params": {
          "ACL": "public-read"
        }
      }
    },
    "crowdin": {
      "crowdinPath": "TODO: UPDATE APP_ID: /apps/APP_ID/dashboard/en.json",
      "localPath": "src/assets/i18n/en.json"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.8",
    "@angular/cdk": "11.1.1",
    "@angular/common": "11.2.8",
    "@angular/compiler": "11.2.8",
    "@angular/core": "11.2.8",
    "@angular/forms": "11.2.8",
    "@angular/material": "11.1.1",
    "@angular/platform-browser": "11.2.8",
    "@angular/platform-browser-dynamic": "11.2.8",
    "@angular/platform-server": "11.2.8",
    "@angular/router": "11.2.8",
    "@enplug/components": "^9.1.1",
    "@enplug/sdk-dashboard": "^5.3.9",
    "@ngneat/transloco": "^2.20.1",
    "@ngneat/transloco-messageformat": "^1.3.1",
    "@ngneat/until-destroy": "^8.0.4",
    "@ngrx/store": "^11.1.0",
    "@types/lodash-es": "^4.17.4",
    "fast-deep-equal": "^3.1.3",
    "immer": "^9.0.1",
    "ionicons": "3",
    "rxjs": "6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.7",
    "@angular/cli": "11.2.7",
    "@angular/compiler-cli": "11.2.8",
    "@angular/language-service": "11.2.8",
    "@enplug/scripts": "^1.11.3",
    "@ngneat/transloco-keys-manager": "^2.7.1",
    "@types/jasmine": "^3.6.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "2.2.5",
    "protractor": "~7.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.9.1",
    "ts-node": "8.3.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.7"
  }
}
