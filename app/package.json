{
  "name": "player-app-seed",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "enplug-build --configuration=development",
    "build:staging": "enplug-build --configuration=staging",
    "build:prod": "enplug-build --configuration=production",
    "release": "enplug-release --configuration=development",
    "release:staging": "enplug-release --configuration=staging",
    "release:prod": "enplug-release --configuration=production",
    "release:translations": "enplug-release-translations",
    "start": "ng serve --configuration=local --port=50001",
    "start:dev": "ng serve --configuration=development --port=50001",
    "start:staging": "ng serve --configuration=staging --port=50001",
    "start:prod": "ng serve --configuration=production --port=50001",
    "start:ssl": "enplug-serve --port=50001",
    "test": "ng test",
    "lint": "ng lint && npm run lint:stylelint",
    "lint:fix": "ng lint --fix && npm run lint:stylelint -- --fix",
    "lint:stylelint": "stylelint \"src/**/*.scss\"",
    "e2e": "ng e2e",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find"
  },
  "config": {
    "destination": "enplug/appseed/app/",
    "aws": {
      "buckets": [
        "apps.enplug.in",
        "apps.enplug.com"
      ],
      "s3": {
        "cache": false,
        "overwrite": true,
        "region": "us-west-2",
        "sslEnabled": true,
        "apiVersion": "2016-05-01",
        "params": {
          "ACL": "public-read"
        }
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.4",
    "@angular/common": "10.2.4",
    "@angular/compiler": "10.2.4",
    "@angular/core": "10.2.4",
    "@angular/forms": "10.2.4",
    "@angular/platform-browser": "10.2.4",
    "@angular/platform-browser-dynamic": "10.2.4",
    "@angular/router": "10.2.4",
    "@enplug/components": "7.0.2",
    "@enplug/sdk-player": "0.10.8",
    "@ngneat/transloco": "2.17.1",
    "@ngneat/transloco-messageformat": "1.2.1",
    "rxjs": "6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "10.2.3",
    "@angular/compiler-cli": "10.2.4",
    "@angular/language-service": "10.2.4",
    "@enplug/scripts": "1.11.1",
    "@ngneat/transloco-keys-manager": "2.0.7",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine": "3.4.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "2.2.5",
    "protractor": "~7.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.9.1",
    "ts-node": "8.3.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.7"
  }
}