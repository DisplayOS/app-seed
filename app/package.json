{
  "name": "player-app",
  "version": "1.1.0",
  "engines": {
    "node": ">=9.0.0 <10.0"
  },
  "scripts": {
    "postinstall": "node translation-libs-patch.js",
    "ng": "ng",
    "dev": "ng serve --port=50000",
    "start": "ng serve --port=50000 --ssl",
    "build": "enplug-build",
    "release": "enplug-release",
    "release:dev": "enplug-release --dev",
    "i18n": "ng-xi18n --i18nFormat=xlf",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "config": {
    "destination": "enplug/appseed/app/",
    "aws": {
      "buckets": [
        "apps.enplug.in",
        "apps.enplug.com"
      ],
      "s3": {
        "cache": false,
        "overwrite": true,
        "region": "us-west-2",
        "sslEnabled": true,
        "apiVersion": "2016-05-01",
        "params": {
          "ACL": "public-read"
        }
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/router": "6.1.10",
    "@biesbjerg/ngx-translate-po-http-loader": "^3.1.0",
    "@enplug/sdk-player": "^0.10.3",
    "@ngx-translate/core": "^10.0.2",
    "core-js": "^2.5.7",
    "rxjs": "6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.3",
    "@angular/cli": "6.2.5",
    "@angular/compiler-cli": "6.1.10",
    "@angular/language-service": "6.1.10",
    "@enplug/scripts": "^1.6.9",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^10.12.0",
    "codelyzer": "~4.2.1",
    "jasmine": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "^5.4.1",
    "karma-mocha-reporter": "^2.2.5",
    "s3": "^4.4.0",
    "ts-node": "^6.1.0",
    "tslint": "^5.11.0",
    "typescript": "~2.9.2"
  }
}
